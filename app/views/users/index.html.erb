<h1>メッセージ 一覧</h1>
<%= link_to "投稿画面へ", new_user_path %>
<br><% @users.each do |user| %>
	<p>
		<%= user.title %>
		<%= link_to "詳細", user_path(user.id)%>
		<%= link_to "編集", edit_user_path(user), method: :get %>
		<%= link_to "削除", user_path(user), method: :delete, data: {confirm: "削除しますか?"} %>
	</p>
<% end %>